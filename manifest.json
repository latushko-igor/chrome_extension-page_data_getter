{
    "name": "Page Data Getter",
    "description": "Quickly copying custom format page data",
    "version": "0.1",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/icon_16.png",
            "48": "/images/icon_48.png",
            "128": "/images/icon_128.png"
        }
    },
    "icons": {
        "16": "/images/icon_16.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png"
    }
}